.container {
  width: 100vw;
  height: 100vh;
  background-color: #eeeeee;
  display: flex;
  flex-direction: column;
}
.logo {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;

}
.nick-name {

}
.footer {
  margin: 40rpx auto;
  text-align: center;
  width: 80%;
}
.tips {
  font-size: 24rpx;
  color: #c8c9cc;
  margin-top: 20rpx;
  text-align: left;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #aaa;
  width: 80%;
}
.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}